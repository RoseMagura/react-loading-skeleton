{
    "name": "react-loading-skeleton",
    "version": "2.1.1",
    "keywords": [
        "react",
        "loading",
        "skeleton",
        "progress",
        "spinner"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dvtng/react-loading-skeleton.git"
    },
    "license": "MIT",
    "author": "David Tang",
    "sideEffects": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.mjs",
    "files": [
        "dist/"
    ],
    "lint-staged": {
        "*.ts?(x)": [
            "eslint --max-warnings 0 --fix",
            "prettier --write"
        ],
        "*.{md,js,cjs,yml,json}": "prettier --write"
    },
    "scripts": {
        "build": "yarn clean && rollup -c .config/rollup.config.js",
        "clean": "rimraf dist",
        "prepack": "yarn run build",
        "prettier-all": "prettier .",
        "start": "yarn run storybook",
        "storybook": "start-storybook -p 6006",
        "test": "jest --config .config/jest.config.cjs",
        "lint": "eslint",
        "lint-all": "yarn lint .",
        "lint-staged": "lint-staged --no-stash",
        "build-storybook": "build-storybook",
        "setup": "husky install"
    },
    "browserslist": [
        "maintained node versions",
        "> 0.25%",
        "not dead",
        "not IE 11"
    ],
    "devDependencies": {
        "@babel/core": "^7.15.8",
        "@mdx-js/react": "^1.6.22",
        "@rollup/plugin-typescript": "^8.2.5",
        "@storybook/addon-actions": "^6.3.10",
        "@storybook/addon-docs": "^6.3.10",
        "@storybook/addon-essentials": "^6.3.10",
        "@storybook/addon-links": "^6.3.10",
        "@storybook/react": "^6.3.10",
        "@swc/core": "^1.2.93",
        "@swc/jest": "^0.2.4",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.1.2",
        "@types/react": "^17.0.27",
        "@types/react-dom": "^17.0.9",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "eslint": "^7.32.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-flowtype": "^6.1.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jest": "^24.5.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-testing-library": "^4.12.4",
        "husky": "^7.0.0",
        "jest": "^27.2.4",
        "lint-staged": "^11.2.0",
        "prettier": "^2.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rimraf": "^3.0.2",
        "rollup": "^2.58.0",
        "rollup-plugin-copy": "^3.4.0",
        "typescript": "^4.4.3"
    },
    "peerDependencies": {
        "react": ">=16.8.0"
    },
    "packageManager": "yarn@3.0.2"
}
